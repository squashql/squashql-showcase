  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Launch ShowcaseApplication",
      "request": "launch",
      "mainClass": "io.squashql.ShowcaseApplication",
      "projectName": "squashql-showcase",
      "vmArgs": "--add-opens=java.base/sun.nio.ch=ALL-UNNAMED"
    }
  ]
}
